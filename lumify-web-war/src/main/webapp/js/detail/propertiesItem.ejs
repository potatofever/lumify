<tr class="property property-<%= property.key %>">

  <td class="property-name"><strong><%= property.displayName %></strong></td>

  <td class="property-value">

    <% if (property.key === '_visibilityJson') { %>

        <span class="visibility entity-visibility" data-visibility="<%= JSON.stringify(property.value) %>"></span>
        <button data-property="<%= JSON.stringify(property) %>" class="info"></button>

    <% } else { %>

      <% if (property.key === 'geoLocation' && property.value) { %>

        <% if (popout) { %>
          <span>
        <% } else { %>
          <a href="#" class="map-coordinates" data-latitude="<%= property.value.value.latitude %>" data-longitude="<%= property.value.value.longitude %>">
        <% } %>

        <%= (property.value._geoLocationDescription && property.value._geoLocationDescription.value) || (property.value.value.latitude + ', ' + property.value.value.longitude) %>

        <% if (popout) { %>
          </span>
        <% } else { %>
          </a>
        <% } %>

      <% } else { %>
        <%= property !== undefined && property !== null ? property.value : '' %>
      <% } %>

        <button data-property="<%= JSON.stringify(property) %>" class="info"></button>
        <span class="visibility" data-visibility="<%= JSON.stringify(_.isUndefined(property.visibility) ? '' : property.visibility) %>"></span>

    <% } %>

  </td>
</tr>
